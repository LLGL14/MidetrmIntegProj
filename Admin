import java.util.*;

public class AdminModuleFunctionalities{
	public static void main(String[] args){
		Scanner kbd = new Scanner(System.in);
		int choose = 0;

		while(choose <= 0 || choose >=7){
			System.out.println("Enter a number you want to execute:");
			System.out.println("1. Start the Server");
			System.out.println("2. Register");
			System.out.println("3. Create project");
			System.out.println("4. View Projects");
			System.out.println("5. Stop the Server");
			System.out.println("6. Exit");
			
			Switch(choose){
				case 1:
					break;
				case 2:
					break;
				case 3:
					break;
				case 4:
					break;
				case 5:
				 	break;
				 case 6:
				 	break;
			}
		}
	}
}
